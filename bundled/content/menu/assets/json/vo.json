[{"id":"vo-0","sound":"audio/vo/vo-0","text":"Welcome to Get Creative!"},{"id":"vo-1","sound":"audio/vo/vo-1","text":"Which CBeebies friend do you want to play with today?"},{"id":"vo-2","sound":"audio/vo/vo-2","text":"Swipe left and right or use the buttons to look at the characters."},{"id":"vo-3","sound":"audio/vo/vo-3","text":"Tap on the play button to choose them"},{"id":"vo-4","sound":"audio/vo/vo-4","text":"OK, let's go and Get Creative!"},{"id":"vo-5","sound":"audio/vo/vo-5","text":"Wheeee!"},{"id":"vo-6","sound":"audio/vo/vo-6","text":"We've arrived... Let's have a quick look around!"},{"id":"vo-7","sound":"audio/vo/vo-7","text":"Tap on the activity boxes to see what's inside them"},{"id":"vo-8","sound":"audio/vo/vo-8","text":"To start with, let's try Magic Paint."},{"id":"vo-9","sound":"audio/vo/vo-9","text":"Press the play button to start"},{"id":"vo-10","sound":"audio/vo/vo-10","text":"You can find all the amazing art you've saved in the gallery"},{"id":"vo-11","sound":"audio/vo/vo-11","text":"Tap on a picture to look at it."},{"id":"vo-12","sound":"audio/vo/vo-12","text":"And press the play button to go and look at it in the activity where you made it"},{"id":"shall_we_challenge_today","sound":"audio/vo/shall_we_challenge_today","text":""},{"id":"click_stars_near_cart_challenge","sound":"audio/vo/click_stars_near_cart_challenge","text":""},{"id":"factory-return-user","sound":"audio/vo/factory-return-user","text":""},{"id":"gallery-welcome","sound":"audio/vo/gallery-welcome","text":""},{"id":"gallery-first-time","sound":"audio/vo/gallery-first-time","text":""},{"id":"lets_have_a_look_gallery","sound":"audio/vo/lets_have_a_look_gallery","text":""},{"id":"rewards_generic1","sound":"audio/vo/rewards_generic1","text":""},{"id":"rewards_generic2","sound":"audio/vo/rewards_generic2","text":""},{"id":"factory-prompt","sound":"audio/vo/factory-prompt","text":""},{"id":"factory-onboard-2","sound":"audio/vo/factory-onboard-2","text":""}]